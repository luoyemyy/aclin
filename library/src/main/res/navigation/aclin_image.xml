<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/aclin_image"
    app:startDestination="@id/galleryFragment">

    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.github.luoyemyy.aclin.image.picker.gallery.GalleryFragment"
        android:label="GalleryFragment" >
        <action
            android:id="@+id/action_galleryFragment_to_previewFragment"
            app:destination="@id/previewFragment" />
        <action
            android:id="@+id/action_galleryFragment_to_cropFragment"
            app:destination="@id/cropFragment" />
    </fragment>
    <fragment
        android:id="@+id/previewFragment"
        android:name="com.github.luoyemyy.aclin.image.preview.PreviewFragment"
        android:label="" />
    <fragment
        android:id="@+id/cropFragment"
        android:name="com.github.luoyemyy.aclin.image.crop.CropFragment"
        android:label="CropFragment" />
</navigation>